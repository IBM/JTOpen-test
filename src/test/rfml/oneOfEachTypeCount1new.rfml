  <data name="field2" type="int" length="2" count="1"/>
  <data name="field3" type="zoned" length="2" count="1"/>
  <data name="field4" type="packed" length="2" count="1"/>
  <data name="field5" type="float" length="4" count="1"/>
  <data name="field6" type="byte" length="2" count="1"/>
  <data name="field7" type="date" dateformat="ISO" dateseparator="hyphen" count="1"/>
  <data name="field8" type="time" timeformat="ISO" timeseparator="period" count="1"/>
  <data name="field9" type="timestamp" count="1"/>
  <data name="fieldA" type="int" length="8" precision="63" count="1"/>
  <data name="fieldB" type="int" length="8" precision="64" count="1"/>
